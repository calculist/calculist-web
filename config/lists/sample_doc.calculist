    {
      "text": "\\emoji[U+1F44B] Hello! This sample document is meant to give you a taste of what Calculist can do.",
      "guid": "99cbdbb5-cd0e-45d2-883b-08978a5fac69",
      "collapsed": null,
      "sort_order": 100.0,
      "items": []
    },
    {
      "text": "This document is editable. So feel free to play with the examples to try out each feature.",
      "guid": "fc9be2d2-a948-4eb7-a837-d2d233fbe18b",
      "collapsed": false,
      "sort_order": 150.0,
      "items": []
    },
